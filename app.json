{
  "expo": {
    "jsEngine": "jsc",
    "name": "subtitle-sage",
    "slug": "subtitle-sage",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.charlescoeder.subtitlesage"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2f78595e-3cce-44c4-a0dd-2635dbc294b9"
      }
    },
    "owner": "charlescoeder",
    "plugins": [
      "expo-router",
      "expo-build-properties",
      "@morrowdigital/watermelondb-expo-plugin",
      "react-native-vlc-media-player"
    ]
  }
}
